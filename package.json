{
  "name": "abstract",
  "displayName": "Abstract",
  "description": "Explorer for Abstract",
  "version": "0.0.1",
  "publisher": "Abstract",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:abstractExplorer"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "abstractItem.openInWeb",
        "title": "Open in Web"
      },
      {
        "command": "abstractItem.openInApp",
        "title": "Open in App"
      },
      {
        "command": "abstractItem.viewDetails",
        "title": "View Details"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "abstractItem.openInWeb",
          "when": "view == abstractExplorer"
        },
        {
          "command": "abstractItem.openInApp",
          "when": "view == abstractExplorer"
        },
        {
          "command": "abstractItem.viewDetails",
          "when": "view == abstractExplorer"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "abstractActivity",
          "title": "Abstract",
          "icon": "resources/abstract.svg"
        }
      ]
    },
    "views": {
      "abstractActivity": [
        {
          "id": "abstractExplorer",
          "name": "Explorer"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "abstract-sdk": "0.3.1",
    "json-pretty-html": "1.1.5"
  }
}
